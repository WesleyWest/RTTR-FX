<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="128.0" prefWidth="257.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="forms.Settings.users.RemoveConfirmationController">
   <children>
      <Button fx:id="removeButton" defaultButton="true" disable="true" layoutX="19.0" layoutY="94.0" mnemonicParsing="false" onAction="#removeButtonClick" text="Удалить" />
      <Label layoutX="14.0" layoutY="9.0" text="Удаляется запись [ ]" />
      <Label fx:id="recordLabel" layoutX="14.0" layoutY="35.0" text=" " />
      <TextField fx:id="textField" layoutX="19.0" layoutY="64.0" onKeyReleased="#textFiledChange" prefHeight="25.0" prefWidth="225.0" />
      <Button fx:id="cancelButton" cancelButton="true" layoutX="185.0" layoutY="94.0" mnemonicParsing="false" onAction="#cancelButtonClick" prefHeight="25.0" prefWidth="59.0" text="Отмена" />
      <Label layoutX="14.0" layoutY="27.0" text="Для удаления введите имя пользователя&#10;и нажмите удалить." />
   </children>
</AnchorPane>
